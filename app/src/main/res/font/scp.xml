<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <font
        android:font="@font/scp_light"
        android:fontStyle="normal"
        android:fontWeight="300"

        app:font="@font/scp_regular"
        app:fontStyle="normal"
        app:fontWeight="300"
        tools:targetApi="o" />
    <font
        android:font="@font/scp_light_italic"
        android:fontStyle="italic"
        android:fontWeight="300"

        app:font="@font/scp_italic"
        app:fontStyle="italic"
        app:fontWeight="300"
        tools:targetApi="o" />
    <font
        android:font="@font/scp_regular"
        android:fontStyle="normal"
        android:fontWeight="400"

        app:font="@font/scp_regular"
        app:fontStyle="normal"
        app:fontWeight="400"
        tools:targetApi="o" />
    <font
        android:font="@font/scp_italic"
        android:fontStyle="italic"
        android:fontWeight="400"

        app:font="@font/scp_italic"
        app:fontStyle="italic"
        app:fontWeight="400"
        tools:targetApi="o" />
    <font
        android:font="@font/scp_bold"
        android:fontStyle="normal"
        android:fontWeight="700"

        app:font="@font/scp_bold"
        app:fontStyle="normal"
        app:fontWeight="700"
        tools:targetApi="o" />
    <font
        android:font="@font/scp_bold_italic"
        android:fontStyle="italic"
        android:fontWeight="700"

        app:font="@font/scp_bold_italic"
        app:fontStyle="italic"
        app:fontWeight="700"
        tools:targetApi="o" />
</font-family>
